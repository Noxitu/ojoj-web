<!DOCTYPE html>
<html>
<head>
  <title>Ojoj</title>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
  <div>Signed as <%= current_user.name %> | <%= link_to "Logout", logout_path %></div>
<% else %>
  <div><%= link_to "Login", login_path %> | <%= link_to "Register", register_path %></div>
<% end %>

<%= yield %>

</body>
</html>
